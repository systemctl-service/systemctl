{{content.field_banner}}
    <section id="blog" class="blog-details service-details">
    	<div class="container">
    		<div class="row">
    			<div class="col-lg-10 col-md-10 mx-auto">
    				<div class="blog-box">
    					<div class="hovereffect blogimage wow fadeInUp" data-wow-delay="0.3s">
    				      {{content.field_image}}
               	</div>
      					<div class="blog-caption wow fadeInDown" data-wow-delay="0.5s">
      						<h3>{{ node.title.value }}</h3>
    							{{ content.body }}
      					</div>
    				</div>
    			</div>
    		</div>
    	</div>
    </section> 
