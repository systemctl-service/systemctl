    <!-- About-us -->
    <section id="about">
      <div class="container">
        <div class="row">
          <div class="col-md-9 col-lg-9 mx-auto text-center title-section wow fadeInUp" data-wow-delay="0.3s">
            <h2 class="title wow fadeInRight">{{content.field_title.0}}</h2>
            <p class="wow fadeInLeft">{{content.field_description.0}}</p>
          </div> 
        </div>
        <div class="row">
          <div class="col-md-12 col-lg-6 wow fadeInLeft" data-wow-delay="0.6s">
