  {{content.field_work_banner}}

    <section id="blog" class="blog-details service-details work-details">
    	<div class="container">
    		<div class="row">
    			<div class="col-lg-12 col-md-12">
    				<div class="blog-box">
    					<div class="hovereffect blogimage wow fadeInUp" data-wow-delay="0.3s">
    						{{content.field_image}}
    					</div>
              <div class="row">
                <div class="col-lg-8 col-md-7">
                  <div class="blog-caption wow fadeInDown" data-wow-delay="0.5s">
                    <h3>{{ node.title.value }}</h3>
                    {{ content.body }}
                  </div>
                </div>
                <div class="col-lg-4 col-md-5 project-details">
                  {{content.field_project_details}}
                </div>
              </div>
              {{content.field_others_project}}
    				</div>
    			</div>
    		</div>
    	</div>
    </section> 
